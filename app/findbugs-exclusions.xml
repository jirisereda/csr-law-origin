<FindBugsFilter>
    <!-- Dagger generated code -->
    <Match>
        <Class name="~.*\.Dagger[^.]*$"/>
    </Match>
    <Match>
        <!-- ignore all issues in resource generation -->
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
</FindBugsFilter>
